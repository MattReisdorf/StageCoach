    "bcrypt": "^5.0.1",
    "connect-session-sequelize": "^7.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "if-env": "^1.0.4",
